<?xml version="1.0" encoding="UTF-8"?>
<configuration>
	<appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%d{yyyy-MM-dd HH:mm:ss.SSS} %t %p: %m%n</pattern>
		</encoder>
	</appender>

	<logger name="net.lm" level="DEBUG" />

	<root level="WARN">
		<appender-ref ref="STDOUT" />
	</root>
</configuration>
